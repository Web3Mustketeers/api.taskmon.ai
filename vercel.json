{
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "pnpm prisma:generate && pnpm gen-typings && pnpm build",
  "devCommand": "pnpm start:dev",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ]
}
